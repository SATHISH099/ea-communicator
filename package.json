{
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "format": "prettier --write \"**/*.{ts,vue}\"",
    "format:check": "prettier --check \"**/*.{ts,vue}\"",
    "dev": "nuxt dev",
    "lint": "eslint \"*.{ts,vue}\" --fix",
    "lint:check": "eslint \"*.{ts,vue}\"",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && git config core.hooksPath .githooks && chmod +x .githooks/pre-commit"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.1.1",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@vitest/coverage-c8": "^0.28.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nuxt": "3.0.0",
    "prettier": "^2.8.3",
    "prop-types": "^15.6.0",
    "sass": "^1.57.1",
    "sass-loader": "10",
    "vitest": "^0.28.3"
  },
  "dependencies": {
    "@formkit/nuxt": "1.0.0-beta.12",
    "@nuxt/image": "^0.7.1",
    "@nuxtjs/color-mode": "^3.2.0",
    "@nuxtjs/google-fonts": "^2.0.0",
    "@nuxtjs/html-validator": "^1.2.3",
    "@unocss/nuxt": "^0.48.0",
    "lint-staged": "^13.1.0",
    "pinia": "^2.0.28",
    "vee-validate": "^4.7.3",
    "yup": "^0.32.11"
  },
  "peerDependencies": {
    "prop-types": "^15.6.0"
  }
}
