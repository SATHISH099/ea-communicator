{
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "format": "prettier --write \"**/*.{ts,vue}\"",
    "format:check": "prettier --check \"**/*.{ts,vue}\"",
    "lint": "eslint \"*.{ts,vue}\" --fix",
    "lint:check": "eslint \"*.{ts,vue}\"",
    "type:check": "nuxt typecheck",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "postinstall": "nuxt prepare && git config core.hooksPath .githooks && chmod +x .githooks/pre-commit"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.1",
    "@nuxt/test-utils": "^3.1.1",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@vitest/coverage-c8": "^0.28.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nuxt": "3.0.0",
    "prettier": "^2.8.3",
    "prop-types": "^15.6.0",
    "sass": "^1.57.1",
    "sass-loader": "10",
    "vitest": "^0.28.3",
    "vue-tsc": "^1.0.24"
  },
  "dependencies": {
    "@formkit/nuxt": "1.0.0-beta.12",
    "@nuxt/image": "^0.7.1",
    "@nuxtjs/color-mode": "^3.2.0",
    "@nuxtjs/html-validator": "^1.2.3",
    "@pinia/nuxt": "^0.4.6",
    "@unocss/nuxt": "^0.48.0",
    "@vueform/multiselect": "^2.5.8",
    "@vuepic/vue-datepicker": "^3.6.8",
    "@vueup/vue-quill": "^1.1.0",
    "lint-staged": "^13.1.0",
    "pinia": "^2.0.28",
    "typescript": "^4.9.5",
    "vee-validate": "^4.7.3",
    "yup": "^0.32.11"
  },
  "peerDependencies": {
    "prop-types": "^15.6.0"
  }
}
