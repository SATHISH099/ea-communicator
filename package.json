{
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "format": "prettier --write \"**/*.{ts,vue}\"",
    "format:check": "prettier --check \"**/*.{ts,vue}\"",
    "lint": "eslint \"*.{ts,vue}\" --fix",
    "lint:check": "eslint \"*.{ts,vue}\"",
    "type:check": "nuxt typecheck",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "postinstall": "nuxt prepare && git config core.hooksPath .githooks && chmod +x .githooks/pre-commit"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.36.0",
    "@nuxt/test-utils": "^3.2.3",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@vitest/coverage-c8": "^0.29.2",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nuxt": "3.2.3",
    "prettier": "^2.8.4",
    "prop-types": "^15.8.1",
    "sass": "^1.58.3",
    "sass-loader": "13",
    "serve": "^14.2.0",
    "vitest": "^0.29.2",
    "vue-tsc": "^1.2.0"
  },
  "dependencies": {
    "@formkit/nuxt": "1.0.0-beta.15",
    "@nuxt/image": "^0.7.1",
    "@nuxtjs/color-mode": "^3.2.0",
    "@nuxtjs/html-validator": "^1.2.4",
    "@pinia/nuxt": "^0.4.7",
    "@unocss/nuxt": "^0.50.4",
    "@vueform/multiselect": "^2.5.8",
    "@vuepic/vue-datepicker": "^4.2.0",
    "@vueup/vue-quill": "^1.1.1",
    "lint-staged": "^13.1.2",
    "pinia": "^2.0.33",
    "typescript": "^4.9.5",
    "vee-validate": "^4.7.4",
    "yup": "^1.0.2",
    "zod": "^3.21.4"
  },
  "peerDependencies": {
    "prop-types": "^15.6.0"
  }
}
